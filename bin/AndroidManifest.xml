<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.psiuol21.smashstack"
    android:versionCode="2"
    android:versionName="2.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/title_activity_first"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.smashstack.FirstAct"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.smashstack.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.smashstack.LastActivity"
            android:label="@string/title_activity_last" >
        </activity>
    </application>

</manifest>
